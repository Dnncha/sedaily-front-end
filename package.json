{
  "name": "sedaily-front-end_nuxt",
  "version": "1.0.0",
  "description": "Software Engineering Daily",
  "author": "dfcook <dfcook@hotmail.com>",
  "private": true,
  "scripts": {
    "nuxt": "nuxt",
    "dev": "cross-env API_ENV=development npm run nuxt",
    "dev:api-prod": "cross-env API_ENV=production npm run nuxt",
    "dev:api-test": "cross-env API_ENV=staging npm run nuxt",
    "build": "nuxt build",
    "build:staging": "cross-env API_ENV=staging npm run build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "test": "ava --serial --verbose",
    "precommit": "npm run lint"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "@nuxtjs/auth": "^4.0.0-rc.3",
    "@nuxtjs/axios": "^5.1.1",
    "@nuxtjs/toast": "^3.0.0",
    "axios": "^0.18.0",
    "lodash": "^4.17.5",
    "moment": "^2.21.0",
    "nuxt": "^1.4.0",
    "phoenix": "^1.3.0",
    "vee-validate": "^2.0.5",
    "vue-infinite-scroll": "^2.0.2",
    "vue-slider-component": "^2.5.8",
    "vue-social-sharing": "^2.3.3",
    "vue-stripe-elements-plus": "^0.2.8",
    "webpack-merge": "^4.1.2"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-eslint": "^8.2.2",
    "cross-env": "^5.1.3",
    "eslint": "^4.18.2",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-vue": "^4.3.0",
    "jsdom": "^11.6.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"
  }
}
